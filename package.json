    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "chai": "^4.3.6",
    "chance": "^1.1.8",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "mysql2": "^2.3.3",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1",
    "sequelize": "^6.16.2"
